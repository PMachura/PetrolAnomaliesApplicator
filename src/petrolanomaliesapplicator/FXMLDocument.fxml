<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="600" prefWidth="1000" xmlns:fx="http://javafx.com/fxml/1" fx:controller="petrolanomaliesapplicator.FXMLDocumentController">
    <children>
        <Label layoutX="10" layoutY="5" minHeight="16" minWidth="69" fx:id="tankMeasuresLabel" />
        <Button layoutX="10" layoutY="30" text="Choose file" onAction="#handleLoadTankMeasures" fx:id="loadTankMeasuresButton" />
        <Label layoutX="100" layoutY="30" minHeight="16" minWidth="69" fx:id="tankMeasuresFileLabel" />
        <Separator layoutX="0" layoutY="60" minWidth="1000" /> 
        
        <Label layoutX="10" layoutY="70" minHeight="16" minWidth="50" fx:id="tankLabel" />
        <ChoiceBox layoutX="10" layoutY="90" fx:id="tankChoiceBox" />
        
        <Label layoutX="70" layoutY="70" minHeight="16" minWidth="50" fx:id="constantTankLeakage" />
        <TextField layoutX="70" layoutY="90" fx:id="constantTankLeakageValue" />
        
        <Label layoutX="250" layoutY="70" minHeight="16" minWidth="50" fx:id="dateStartLabel" />
        <TextField layoutX="250" layoutY="90" fx:id="dateStartValue" />
        
        <Label layoutX="550" layoutY="70" minHeight="16" minWidth="50" fx:id="dateEndLabel" />
        <TextField layoutX="550" layoutY="90" fx:id="dateEndValue" />
        
        
        
        <Button layoutX="485" layoutY="500" text="Start applicator" onAction="#handleStartApplicator" fx:id="startApplicatorButton" />
    </children>
</AnchorPane>
